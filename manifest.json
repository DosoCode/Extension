{
  "name": "Digital Literacy toolkit",
  "description": "Gives guided tutorial for gmail",
  "version": "0.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "tabs"],
  "action": {
    "default_popup": "help_popup.html",
    "default_icon": "help_extensions.png"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}

